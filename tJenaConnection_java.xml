<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.1" STATUS="DEVELOPMENT"

		COMPATIBILITY="ALL" AUTHOR="Stefan Scheffler" RELEASE_DATE="20070525A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>semanticWeb/Jena/Connections</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
			NAME="CONNECTION"
			FIELD="CLOSED_LIST"
			NUM_ROW="1">
			<ITEMS DEFAULT="FUSEKI">
				<ITEM NAME="FUSEKI" VALUE="FUSEKI" />
				<ITEM NAME="VIRTUOSO" VALUE="VIRTUOSO" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="HOST_FUSEKI" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" SHOW_IF="CONNECTION == 'FUSEKI'"
			GROUP="ONT">
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="HOST_VIRTUOSO" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" SHOW_IF="CONNECTION == 'VIRTUOSO'"
			GROUP="ONT">
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT_FUSEKI" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" SHOW_IF="CONNECTION == 'FUSEKI'"
			GROUP="ONT">
			<DEFAULT>"3030"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PORT_VIRTUOSO" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" SHOW_IF="CONNECTION == 'VIRTUOSO'"
			GROUP="ONT">
			<DEFAULT>"1111"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATASET" FIELD="TEXT" NUM_ROW="2" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'FUSEKI'">
			<DEFAULT>"ds"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'VIRTUOSO'">
			<DEFAULT>"dba"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'VIRTUOSO'">
			<DEFAULT>"dba"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="QUERY" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'FUSEKI'">
			<DEFAULT>"query"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="UPDATE" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'FUSEKI'">
			<DEFAULT>"update"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DEFAULT_GRAPH_FUSEKI" FIELD="TEXT" NUM_ROW="4" REQUIRED="false"
			GROUP="ONT" SHOW_IF="CONNECTION == 'FUSEKI'"/>
			
		<PARAMETER NAME="DEFAULT_GRAPH_VIRTUOSO" FIELD="TEXT" NUM_ROW="4" REQUIRED="true"
			GROUP="ONT" SHOW_IF="CONNECTION == 'VIRTUOSO'">
			<DEFAULT>"virt:DEFAULT"</DEFAULT>	
		</PARAMETER>

		<PARAMETER NAME="PREFIXES_VIRTUOSO" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="15" NB_LINES="5" SHOW_IF="CONNECTION == 'VIRTUOSO'">
			<ITEMS>
				<ITEM NAME="PREFIX" />
				<ITEM NAME="NAMESPACE" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="PREFIXES_FUSEKI" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="15" NB_LINES="5" SHOW_IF="CONNECTION == 'FUSEKI'">
			<ITEMS>
				<ITEM NAME="PREFIX" />
				<ITEM NAME="NAMESPACE" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="LOGGING" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<!--<IMPORT NAME="fuseki" MODULE="fuseki-server.jar" REQUIRED="true" />-->
			<IMPORT NAME="jena_talend_connector" MODULE="talend_jena_connector.jar" REQUIRED="true" />
			<!--<IMPORT NAME="jena_jdbc" MODULE="virtjdbc3.jar" REQUIRED="true" />
			<IMPORT NAME="jena_virtuoso_own" MODULE="virt_jena_own.jar" REQUIRED="true" />-->
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>

</COMPONENT>
